<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<AssignMessage async="false" continueOnError="false" enabled="true" name="AssignMessage-SetBackendRequest">
    <DisplayName>Set Backend Request</DisplayName>
    <Properties/>
    <Set>
        <Headers>
            <Header name="Content-Type">application/json</Header>
            <Header name="Accept">application/json</Header>
        </Headers>
        <Payload contentType="application/json">
{
    "accountType": "{request.content.accountType}",
    "customerName": "{request.content.customerName}",
    "customerId": "{request.content.customerId}",
    "initialDeposit": "{request.content.initialDeposit}",
    "currency": "{request.content.currency}",
    "branchCode": "{request.content.branchCode}",
    "requestId": "{messageid}",
    "timestamp": "{system.timestamp}"
}
        </Payload>
    </Set>
    <IgnoreUnresolvedVariables>true</IgnoreUnresolvedVariables>
    <AssignTo createNew="false" transport="http" type="request"/>
</AssignMessage>
