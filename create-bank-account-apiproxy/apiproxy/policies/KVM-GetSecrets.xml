<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<KeyValueMapOperations async="false" continueOnError="false" enabled="true" name="KVM-GetSecrets" mapIdentifier="banking-api-secrets">
    <DisplayName>Get Backend Secrets from KVM</DisplayName>
    <Properties/>
    <ExclusiveCache>false</ExclusiveCache>
    <ExpiryTimeInSecs>3600</ExpiryTimeInSecs>
    <Scope>environment</Scope>
    <Get assignTo="private.backend.api.key">
        <Key>
            <Parameter>backend_api_key</Parameter>
        </Key>
    </Get>
    <Get assignTo="private.backend.client.id">
        <Key>
            <Parameter>backend_client_id</Parameter>
        </Key>
    </Get>
    <Get assignTo="private.backend.client.secret">
        <Key>
            <Parameter>backend_client_secret</Parameter>
        </Key>
    </Get>
</KeyValueMapOperations>
